<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cyj.animation" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view_anim.ViewAnimActivity" />
        <activity android:name=".property_anim.PropertyAnimationActivity" />
        <activity android:name=".layout_anim.LayoutAnimationActivity" />
        <activity android:name=".custom_anim.CustomAnimActivity" />
        <activity android:name=".anim_menu.AnimMenuActivity" />
        <activity android:name=".anim_menu.AnimTimerActivity" />
        <activity android:name=".anim_menu.AnimDropMenuActivity" >
        </activity>
    </application>

</manifest>